<div id="<%= dom_id place %>">
  <p>
    <strong>City:</strong>
    <%= place.city %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= CS.countries()[:"#{place.country}"] %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= place.name %>
  </p>

  <p>
    <strong>Photo:</strong>
    <img class="smallpic" src="/uploads/<%= place.photo %>"/>
  </p>

</div>
